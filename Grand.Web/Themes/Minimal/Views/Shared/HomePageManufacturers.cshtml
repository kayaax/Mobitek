<template v-if="hpi.HomePageItems[0].HomePageManufacturers.loading">
    <b-skeleton animation="fade" height="450px"></b-skeleton>
</template>
<div v-else-if="hpi.HomePageItems[0].HomePageManufacturers.items[0] !== undefined" id="HomePageManufacturers">
    <div class="section">
        <b-col cols="12" class="px-0">
            <div class="item-row">
                <b-col xl="3" lg="3" md="3" sm="6" cols="6" v-for="Model in hpi.HomePageItems[0].HomePageManufacturers.items">
                    <a :href="'/' + Model.SeName">
                        <img class="img-fluid" :src="Model.PictureModel.FullSizeImageUrl" :alt="Model.PictureModel.Title" class="gallery__img">
                    </a>
                </b-col>
            </div>
        </b-col>
    </div>
</div>