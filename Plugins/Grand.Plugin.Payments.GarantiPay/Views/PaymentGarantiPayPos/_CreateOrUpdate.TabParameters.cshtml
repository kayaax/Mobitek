                        <div class="col-md-9 col-sm-9">
                            <admin-input asp-for="@Model.ParameterModel.GarantiBankModel.TerminalProvUserId" />
                            <span asp-validation-for="@Model.ParameterModel.GarantiBankModel.TerminalProvUserId"></span>
