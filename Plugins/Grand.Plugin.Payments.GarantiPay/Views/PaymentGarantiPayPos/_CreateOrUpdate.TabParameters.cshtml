{
    <div class="panel panel-default">
        <div class="panel-body">
            <div class="form-horizontal">
                <div class="form-body">
                    <div class="form-group">
                        <admin-label asp-for="@Model.ParameterModel.GarantiBankModel.TerminalId" />
                        <div class="col-md-9 col-sm-9">
                            <admin-input asp-for="@Model.ParameterModel.GarantiBankModel.TerminalId" />
                            <span asp-validation-for="@Model.ParameterModel.GarantiBankModel.TerminalId"></span>
                        </div>
                    </div>
                    <div class="form-group">
                        <admin-label asp-for="@Model.ParameterModel.GarantiBankModel.TerminalUserId" />
                        <div class="col-md-9 col-sm-9">
